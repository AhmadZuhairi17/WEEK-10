package myPractice;

class ArrayPractice4 {

	public static void main(String[] args) {
		double[] numArray = { 34.2,46.2,50.1,28.7,33.1 };
        double sum = 0.0;

        for (double num: numArray) {
           sum += num;
        }

        double average = sum / numArray.length;
        System.out.format("The average is: %.2f", average);

	}

}
